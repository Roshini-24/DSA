# bubble sort
class Solution:
    #Function to sort the array using bubble sort algorithm.
    def bubbleSort(self,arr, n):
        for i in range(n-1):
            swap= 0
            for j in range(n-i-1):
                if arr[j]>arr[j+1]:
                    arr[j],arr[j+1]=arr[j+1],arr[j]
                    swap=1
            if swap==0:
                return arr
        return arr    
# insertion
class Solution:
    def insert(self, alist, n):
        for i in range(1 ,n):
            x=alist[i]
            j=i-1
            while j>=0 and alist[j]>x:
                alist[j+1]=alist[j]
                j=j-1
            alist[j+1]=x
        return alist
    #Function to sort the list using insertion sort algorithm.    
    def insertionSort(self, alist, n):
        
        self.insert(alist,n)
# union
def Union(a, b, n, m):
    result = [0 for _ in range(n + m)]
 
	index, left, right = 0, 0, 0
    while left < n and right < m:
 
        if (a[left] < b[right]):
 
            if(index != 0 and a[left] == result[index-1]):
                left += 1
            else:
                result[index] = a[left]
                left += 1
                index += 1
 
        else:
            if (index != 0 and b[right] == result[index-1]):
 
                right += 1
            else:
                result[index] = b[right]
                right += 1
                index += 1
 
    while(left < n):
        if(index != 0 and a[left] == result[index-1]):
            left += 1
        else:
            result[index] = a[left]
            left += 1
            index += 1
 
    while(right < m):
        if(index != 0 and b[right] == result[index - 1]):
            right += 1
        else:
            result[index] = b[right]
            right += 1
            index += 1
 
    print("Union:", *result[:index])
