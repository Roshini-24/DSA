#Display list
class Node:
    def __init__(self,data):
        self.data=data
        self.next=None

def displayList(head):
    l=[]
    while head:
        l.append(head.data)
        head=head.next
    return l
#count node
 def getCount(self, head_node):
        c=head_node
        d=0
        while c!= None:
            d=d+1
            c=c.next
        return d
#sum of nodes
def sumOfElements(head):
    c=head
    d=0
    while c!= None:
        d=d+c.data
        c=c.next
    return d
#search
def searchLinkedList(head,x):
    c=head
    while c!=None:
        if c.data==x:
            return 1
        c=c.next
    return 0
#delete strat
def deleteHead(head):
    c=head
    if c==None:
        return None
    else:
        head=c.next
        c.next=None
        return head
#delete end
def deleteTail(head):
    if head == None:
        return  None

    if head.next == None:
        return None

    curr = head

    while curr.next.next != None:
        curr = curr.next

    curr.next = None
    return head
#delete pos
def deleteAtPosition(head, pos):
    if head==None:
        return None
    elif pos==1:
        return head.next
    c=head
    for i in range(pos-2):
        c=c.next
    c.next=c.next.next
    return head
